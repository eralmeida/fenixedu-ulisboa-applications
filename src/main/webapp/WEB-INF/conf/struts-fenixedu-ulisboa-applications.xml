<!DOCTYPE struts-config PUBLIC
      "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
      "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
      <!-- ========== Data Source Configuration =============================== -->
      <data-sources></data-sources>
      <!-- ========== Form Bean Definitions ================================== -->
      <form-beans type="org.apache.struts.action.ActionFormBean"></form-beans>
      <!-- ========== Controller Configuration ================================ -->
      <controller processorClass="org.fenixedu.bennu.struts.SimpleRenderersRequestProcessor"></controller>
      <!-- ========== Message Resources Definitions ============================ -->
      <message-resources null="false" key="ULISBOA_APPLICATIONS_RESOURCES" parameter="resources.FenixeduULisboaApplicationsResources"></message-resources>
      <!-- ========== Plug Ins Configuration ================================= -->
      <plug-in className="org.apache.struts.validator.ValidatorPlugIn" />
      <plug-in className="org.fenixedu.bennu.struts.plugin.StrutsAnnotationsPlugIn"></plug-in>
</struts-config>
